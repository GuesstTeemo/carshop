package cn.itcast.shop.advertisement.service;

import java.util.List;

import cn.itcast.shop.advertisement.dao.AdvertisementDao;
import cn.itcast.shop.advertisement.vo.Advertisement;

public class AdvertisementService {
	private AdvertisementDao advertisementDao;

	public void setAdvertisementDao(AdvertisementDao advertisementDao) {
		this.advertisementDao = advertisementDao;
	}

	public void update(Advertisement advertisement) {
		advertisementDao.update(advertisement);
	}

	public Advertisement findpic() {
		return advertisementDao.findpic();
	}


	
}
