package cn.itcast.shop.utils;

import java.util.UUID;

/**
 * 生成随机字符串的工具类
 *    
 * 项目名称：shop00   
 * 类名称：UUIDUtils   
 * 类描述：   
 * 创建人：Teemo   
 * 创建时间：2017年4月19日 下午4:40:25   
 * 修改人：Teemo   
 * 修改时间：2017年4月19日 下午4:40:25   
 * 修改备注：   
 * @version    
 *
 */
public class UUIDUtils {
	/**
	 * 获得随机的字符串
	 * @return
	 */
	public static String getUUID(){
		return UUID.randomUUID().toString().replace("-", "");
	}
}
